{
    "version": 2,
    "builds": [
      {
        "src": "care_connect_backend/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      }
    ],
    "routes": [
      {
        "src": "/media/(.*)",
        "dest": "care_connect_backend/wsgi.py"
      },
      {
        "src": "/(.*)",
        "dest": "care_connect_backend/wsgi.py"
      }
    ]
  }